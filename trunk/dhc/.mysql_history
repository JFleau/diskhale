show tables
;
insert into retards SELECT  trigramme, dateemprunt FROM emprunts WHERE 365*YEAR(dateemprunt)+30*MONTH(dateemprunt)+DAYOFMONTH(dateemprunt)+30<731526 AND daterendu=0
;
select * from retards
;
SELECT trigramme, MIN(dateemprunt), COUNT(trigramme), YEAR(MIN(dateemprunt)), MONTH(MIN(dateemprunt)), DAYOFMONTH(MIN(dateemprunt)) FROM retards GROUP BY trigramme
;
SELECT trig, MIN(dateemprunt), COUNT(trig), YEAR(MIN(dateemprunt)), MONTH(MIN(dateemprunt)), DAYOFMONTH(MIN(dateemprunt)) FROM retards GROUP BY trig
;
SELECT trig, MIN(dat), COUNT(trig), YEAR(MIN(dat)), MONTH(MIN(dat)), DAYOFMONTH(MIN(dat)) FROM retards GROUP BY trig
;
drop table retards
;
CREATE TEMPORARY TABLE retards (trig char(3), dat date) INSERT INTO retards SELECT trigramme, dateemprunt FROM emprunts WHERE 365*YEAR(dateemprunt)+30*MONTH(dateemprunt)+DAYOFMONTH(dateemprunt)+30<731526AND daterendu=0
;
